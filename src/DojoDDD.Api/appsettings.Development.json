{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "DbContext": {
    "Name" : "dojo_ddd"
  },
  "Connections" : {
    "Redis": {
      "Host": "localhost",
      "Prefixes": {
        "Products" : "products:",
        "Hangfire" : "orders_scheduler:"
      }
    },
    "RavenDb": {
      "Endpoints": [
        "http://localhost:8080"
      ],
      "Certificate": "",
      "Password": ""
    },
    "RabbitMq": {
      "Host": "rabbitmq://localhost",
      "Nodes": [
        "localhost"
      ],
      "Username": "guest",
      "Password": "guest"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "Elasticsearch",
        "Args": {
          "indexFormat": "dojo.ddd.api-{0:yyyy.MM}",
          "nodeUris": "http://localhost:9200"
        }
      }
    ]
  },
  "ElasticApm": {
    "ServerUrls": "http://localhost:8200"
  }
}
